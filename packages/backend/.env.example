# LMStudio Configuration
# Set to 'lmstudio' to use local LMStudio server, or 'openai' for OpenAI API
AI_PROVIDER=lmstudio

# LMStudio Settings (when AI_PROVIDER=lmstudio)
LMSTUDIO_BASE_URL=http://localhost:1234/v1
LMSTUDIO_MODEL=gemma3:1b
# LMStudio doesn't require a real API key, but the OpenAI client expects one
LMSTUDIO_API_KEY=lm-studio

# OpenAI Settings (when AI_PROVIDER=openai)
OPENAI_KEY=your_openai_key_here
OPENAI_MODEL=gpt-3.5-turbo
